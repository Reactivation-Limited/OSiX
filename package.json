{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js --roots src",
    "test:inspect": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testTimeout=60000 --collectCoverage=false --roots src",
    "prepare": "husky",
    "install": "swig -c++ -o OSX.cxx -javascript -node OSX.i && node-gyp rebuild",
    "format": "prettier --write './**/*.{js,mjs,cjs,jsx,json,md}'",
    "lint": "eslint src"
  },
  "dependencies": {
    "colors": "1.4.0",
    "debug": "4.3.4",
    "node-addon-api": "*",
    "node-gyp": "10.1.0"
  },
  "author": "",
  "license": "ISC",
  "gypfile": true,
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "eslint": "8.57.0",
    "eslint-plugin-jest": "28.5.0",
    "globals": "15.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs}": [
      "eslint --fix"
    ],
    "**/*.{js,mjs,cjs,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
